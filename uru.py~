def is_leap_year(year):
    if year % 4 == 0:
        if year % 100 == 0:
            if year % 400 == 0:
                return True
            else:
                return False
        else:
            return True
    else:
        retrn False

        for year in range(1950, 2051):
            result =is_leap_year(year)
            print(f"{year}{result}")
